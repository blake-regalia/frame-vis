
frame {

	class: frame;
	frame-old: $class '-old';
	frame-new: $class '-new';

	box {
		width: 200px;
		height: $width * 0.25;
	}
	

	# / [class|=$class] {
		position: relative;

		width: $box.width;
		height: $box.height;
	}


	# / .$frame-new {
		border: 1px solid black;
	}


	# / .$frame-old {
		border: 1px solid grey;
	}
}